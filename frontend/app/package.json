{
    "name": "routify-starter",
    "private": true,
    "scripts": {
        "prepare": "smui-theme compile src/smui.css -i src/theme",
        "dev": "run-p dev:*",
        "dev:routify": "routify",
        "dev:vite": "vite",
        "preview": "vite preview",
        "build": "run-s build:*",
        "build:routify": "routify -b",
        "build:vite": "vite build",
        "generate-client": "openapi --input http://localhost:8000/api/openapi.json --output ./src/client --client axios"
    },
    "devDependencies": {
        "@roxi/routify": "^2.18.4",
        "@smui/button": "^6.0.0-beta.16",
        "@smui/dialog": "^6.0.0-beta.16",
        "@smui/textfield": "^6.0.0-beta.16",
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.34",
        "autoprefixer": "^10.4.2",
        "form-data": "^4.0.0",
        "npm-run-all": "^4.1.5",
        "openapi-typescript-codegen": "^0.22.0",
        "postcss": "^8.4.5",
        "postcss-import": "^14.0.2",
        "smui-theme": "^6.0.0-beta.16",
        "svelte": "^3.45.0",
        "svelte-preprocess": "^4.10.1",
        "vite": "^2.7.10"
    },
    "dependencies": {
        "axios": "^0.27.0",
        "svelte-forms": "^2.3.1",
        "svelte-material-ui": "^6.0.0-beta.16",
        "url": "^0.11.0"
    }
}
